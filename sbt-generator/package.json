{
  "name": "nosocial-sbt-generator",
  "version": "1.0.0",
  "description": "ts examples in how to call the API",
  "license": "GPL-3.0",
  "scripts": {
    "debug": "env-cmd -f .env nodemon src/main.ts",
    "start": "cp -r src/abis build/src && env-cmd -f .env node --trace-warnings build/src/main.js",
    "build": "tsc -p ./",
    "generate": "graphql-codegen",
    "profiles": "env-cmd -f .env ts-node ./src/operation/get-profiles",
    "publications": "env-cmd -f .env ts-node ./src/operation/get-publications",
    "test": "env-cmd -f .env ts-node ./src/indexer/index",
    "test-mint": "nodemon src/operation/mint-nft.ts",
    "explore:explore-publications": "env-cmd -f .env ts-node ./src/explore/explore-publications"
  },
  "authors": [
    "qiufan_glamour@hotmail.com"
  ],
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@openzeppelin/contracts": "^4.8.0",
    "axios": "^0.26.1",
    "bluebird": "^3.7.2",
    "cross-fetch": "^3.1.5",
    "dayjs": "^1.10.6",
    "lodash": "^4.17.20",
    "mongodb": "4.12",
    "omit-deep": "^0.3.0",
    "typestub-ipfs-only-hash": "^4.0.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.7",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.3",
    "@openzeppelin/hardhat-upgrades": "^1.21.0",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/bluebird": "^3.5.36",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "chai": "^4.3.7",
    "env-cmd": "^10.1.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.4",
    "hardhat-gas-reporter": "^1.0.9",
    "nodemon": "^2.0.6",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.4"
  }
}