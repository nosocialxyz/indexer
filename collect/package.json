{
  "name": "lens-hub",
  "version": "1.0.0",
  "description": "ts examples in how to call the API",
  "license": "GPL-3.0",
  "scripts": {
    "debug": "env-cmd -f .env nodemon src/main.ts",
    "start": "cp -r src/abis build/src && env-cmd -f .env node --trace-warnings build/src/main.js",
    "build": "tsc -p ./",

    "generate": "graphql-codegen",
    "profiles": "env-cmd -f .env ts-node ./src/operation/get-profiles",
    "publications": "env-cmd -f .env ts-node ./src/operation/get-publications",
    "test": "env-cmd -f .env ts-node ./src/indexer/index",
    "explore:explore-publications": "env-cmd -f .env ts-node ./src/explore/explore-publications"
  },
  "authors": [
    "ariescodescream@gmail.com"
  ],
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@graphql-codegen/fragment-matcher": "^3.3.1",
    "@lens-protocol/sdk-gated": "^1.1.0",
    "apollo-link-timeout": "^4.0.0",
    "web3": "^1.8.1",
    "lodash": "^4.17.20",
    "axios": "^0.26.1",
    "cross-fetch": "^3.1.5",
    "ethers": "^5.7.2",
    "graphql": "^16.3.0",
    "ipfs-http-client": "^56.0.1",
    "mongodb": "4.12",
    "omit-deep": "^0.3.0",
    "typestub-ipfs-only-hash": "^4.0.0",
    "dayjs": "^1.10.6",
    "winston": "^3.3.3",
    "bluebird": "^3.7.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.11.3",
    "@graphql-codegen/typed-document-node": "2.3.2",
    "@graphql-codegen/typescript": "2.7.2",
    "@graphql-codegen/typescript-operations": "2.5.2",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "@types/bluebird": "^3.5.36",
    "@types/lodash": "^4.14.181",
    "nodemon": "^2.0.6",
    "env-cmd": "^10.1.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.9.3"
  }
}
